<body>
  <div id="loader">
    <div id="loader-emojis"></div>
    <div id="neumorphism-box"></div>
  </div>
  <header>
    <div id="flags">
      <a href='fr' id="france-flag"><img src="src/flags/france.png" class="flag" alt="France Flag"></img></a>
      <a href="en" id="uk-flag"><img src="src/flags/united-kingdom.png" class="flag" alt="English Flag"></img></a>
    </div>
    <div id="title-illustration">
      <h1 id="title">
        <span id="o" class=letters>O</span>
        <span id="u" class=letters>U</span>
        <span id="a" class=letters>A</span>
        <span id="i" class=letters>I</span>
        <span id="z" class=letters>Z</span>
        <span id="e" class=letters>E</span>
        <span id="u2" class=letters>U</span>
        <span id="r" class=letters>R</span>
      </h1>
